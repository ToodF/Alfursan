
@if (ViewData["success"] != null)
{
    <div class="alert alert-success" role="alert">@{WriteLiteral(ViewData["success"]);}</div>
}
@if (ViewData["info"] != null)
{
    <div class="alert alert-info" role="alert">@{WriteLiteral(ViewData["info"]);}</div>
}
@if (ViewData["warning"] != null)
{
    <div class="alert alert-warning" role="alert">@{WriteLiteral(ViewData["warning"]);}</div>
}
@if (ViewData["danger"] != null)
{
    <div class="alert alert-danger" role="alert">@{WriteLiteral(ViewData["danger"]);}</div>
}