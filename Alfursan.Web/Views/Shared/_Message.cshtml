
@if (ViewData["success"] != null)
{
    <div class="alert alert-success" role="alert">@ViewData["success"]</div>
}
@if (ViewData["info"] != null)
{
    <div class="alert alert-info" role="alert">@ViewData["info"]</div>
}
@if (ViewData["warning"] != null)
{
    <div class="alert alert-warning" role="alert">@ViewData["warning"]</div>
}
@if (ViewData["danger"] != null)
{
    <div class="alert alert-danger" role="alert">@ViewData["danger"]</div>
}